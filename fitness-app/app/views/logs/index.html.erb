<h1>Breakfast</h1>
<% @subLog =  @logs.where(day: @currentTimeReal, meal:'Breakfast')  %>
<% @subLog.each do |log| %>
<%= log['meal']  %>
<%= log['calories'] %><br>
<%= log['day'] %><br>
<% end %>

